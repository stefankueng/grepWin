<?xml version="1.0"?>
<project name="SetVersionInfo" default="setversioninfo">
	<target name="setversioninfo">
		<setenv>
			<variable name="MajorVersion" value="1" />
			<variable name="MinorVersion" value="4" />
			<variable name="MicroVersion" value="1" />
			<variable name="WCREV" value="$WCREV$" />
			<variable name="WCDATE" value="$WCDATE$" />
		</setenv>
	</target>
</project>